    ViewBag.Canonical = "https://www.music4dance.net/faq";
    ViewBag.Description = "Frequently Asked Questions about Music4Dance and the answers.";

    ViewBag.BreadCrumbs = BreadCrumbItem.BuildInfoTrail(ViewBag.Title);
}

<div id="app"></div>

@section Scripts {
    <script asp-src-include="/client/js/chunk-vendors.*.js"></script>
    <script asp-src-include="/client/js/chunk-common.*.js"></script>
    <script asp-src-include="/client/js/faq.*.js"></script>
}
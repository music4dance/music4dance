{
  "name": "m4d-client",
  "type": "module",
  "version": "1.0.0",
  "private": true,
  "scripts": {
    "dev": "vite",
    "build": "yarn type-check && yarn build-only",
    "full-build": "run-p type-check \"build-only {@}\" --",
    "preview": "vite preview",
    "test:unit": "vitest",
    "test:ci": "vitest run --reporter=default --reporter=junit --outputFile=reports/junit.xml",
    "test:ui": "vitest --ui --api 9527",
    "build-only": "vite build",
    "type-check": "vue-tsc --build --force",
    "lint": "eslint --fix src",
    "format": "prettier --write src/"
  },
  "dependencies": {
    "@vueuse/core": "^13.9.0",
    "@vueuse/integrations": "^13.9.0",
    "axios": "^1.12.2",
    "bootstrap": "^5.3.8",
    "bootstrap-icons": "^1.13.1",
    "bootstrap-vue-next": "^0.40.5",
    "date-fns": "^4.1.0",
    "focus-trap": "^7.6.5",
    "jquery": "^3.7.1",
    "jquery-validation": "^1.21.0",
    "jquery-validation-unobtrusive": "^4.0.0",
    "markdowndeep": "^1.6.12",
    "typedjson": "^1.8.0",
    "uuid": "^11.1.0",
    "vue": "^3.5.21",
    "vue-showdown": "^4.2.0"
  },
  "devDependencies": {
    "@iconify-json/bi": "^1.2.6",
    "@tsconfig/node18": "^18.2.4",
    "@types/bootstrap": "^5.2.10",
    "@types/jquery": "^3.5.33",
    "@types/jsdom": "^21.1.7",
    "@types/node": "^24.5.2",
    "@types/uuid": "^10.0.0",
    "@typescript-eslint/utils": "^8.44.1",
    "@vitejs/plugin-vue": "^6.0.1",
    "@vitest/eslint-plugin": "^1.3.12",
    "@vitest/ui": "^3.2.4",
    "@vue/compiler-dom": "^3.5.21",
    "@vue/compiler-sfc": "^3.5.21",
    "@vue/eslint-config-prettier": "^10.2.0",
    "@vue/eslint-config-typescript": "^14.6.0",
    "@vue/server-renderer": "^3.5.21",
    "@vue/test-utils": "^2.4.6",
    "@vue/tsconfig": "^0.7.0",
    "eslint": "^9.36.0",
    "eslint-plugin-vue": "^10.5.0",
    "fast-glob": "^3.3.3",
    "jiti": "^2.6.0",
    "jsdom": "^26.1.0",
    "npm-run-all2": "^8.0.4",
    "prettier": "^3.6.2",
    "sass": "^1.93.1",
    "typescript": "~5.9.2",
    "unplugin-icons": "^22.3.0",
    "unplugin-vue-components": "^29.1.0",
    "vite": "^7.1.7",
    "vite-plugin-inspect": "^11.3.3",
    "vite-plugin-mkcert": "^1.17.8",
    "vitest": "^3.2.4",
    "vue-eslint-parser": "^10.2.0",
    "vue-tsc": "^3.0.8"
  },
  "resolutions": {
    "strip-ansi": "6.0.1",
    "string-width": "4.2.2",
    "wrap-ansi": "7.0.0"
  },
  "packageManager": "yarn@4.9.2"
}
